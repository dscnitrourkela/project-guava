{
  "name": "project-guava",
  "version": "0.1.0",
  "description": "The project repository for an open system to generate and manage certificates.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prettier": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "lint:fix": "npx eslint --fix '**/*.js'",
    "lint:find": "npx eslint '**/*.js",
    "start": "npx pm2 start ecosystem.config.cjs --only dev --no-autorestart",
    "stop": "npx pm2 stop ecosystem.config.cjs --only dev",
    "delete": "npx pm2 delete ecosystem.config.cjs --only dev",
    "restart": "npx pm2 restart ecosystem.config.cjs --only dev",
    "logs": "npx pm2 logs",
    "status": "npx pm2 ps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dscnitrourkela/project-guava.git"
  },
  "keywords": [
    "certificate",
    "graphql",
    "mongo",
    "nodejs",
    "automate"
  ],
  "author": "dscnitrourkela",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dscnitrourkela/project-guava/issues"
  },
  "homepage": "https://github.com/dscnitrourkela/project-guava#readme",
  "dependencies": {
    "apollo-server-express": "^2.19.2",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "graphql": "^15.4.0",
    "graphql-iso-date": "^3.6.1",
    "mongoose": "^5.11.13",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-extended": "^0.11.5",
    "pm2": "^5.0.4",
    "prettier": "^2.2.1"
  }
